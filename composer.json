{
  "name": "laravel-dashboard-kit/dashboard-abstract",
  "description": "DashboardAbstract package for Laravel dashboard kit",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amr Khaled",
      "email": "i@akhaled.com"
    }
  ],
  "require": {
    "laravel/ui": "^3.3",
    "spatie/laravel-blade-javascript": "^2.6",
    "livewire/livewire": "^2.5",
    "mediconesystems/livewire-datatables": "^0.5.1",
    "asantibanez/livewire-select": "^2.0",
    "akhaled/livewire-sweetalert": "^0.1",
    "blade-ui-kit/blade-icons": "^1.1",
    "brunocfalcao/blade-feather-icons": "^2.0",
    "ryangjchandler/blade-tabler-icons": "^1.1",
    "andreiio/blade-remix-icon": "^1.0",
    "codeat3/blade-eos-icons": "^1.7",
    "codeat3/blade-carbon-icons": "^1.0",
    "mckenziearts/laravel-notify": "^2.2",
    "illuminate/database": "*",
    "illuminate/support": "*",
    "illuminate/view": "*"
  },
  "autoload": {
    "psr-4": {
      "LDK\\DashboardAbstract\\": "src",
      "LDK\\DashboardAbstract\\Database\\Factories\\": "database/factories",
      "LDK\\DashboardAbstract\\Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "./helpers/all.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "LDK\\DashboardAbstract\\Providers\\DashboardAbstractServiceProvider"
      ]
    }
  }
}
